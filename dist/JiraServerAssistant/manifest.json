{
    "name": "Jira Server Assistant",

    "description": "",

    "version": "1.0",

    "manifest_version": 3,
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "assets/corsRule.json"
