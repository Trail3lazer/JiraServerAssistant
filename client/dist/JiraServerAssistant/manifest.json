{
    "name": "Jira Server Assistant",

    "description": "",

    "version": "1.0",

    "manifest_version": 3,
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "assets/corsRule.json"
            }
        ]
    },
    "permissions": ["declarativeNetRequest", "cookies", "storage"],

    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": ["https://*/"],
    "icons": 
       { "128": "assets/thumbnail 128x128.png"}
    
}
